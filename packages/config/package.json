{
  "name": "@devkitvue/config",
  "version": "1.2.0",
  "type": "module",
  "private": false,
  "main": "./dist/index.es.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "style/",
    "./tsconfig.json",
    "./tsconfig-vue.json"
  ],
  "scripts": {
    "build": "vite build"
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.es.js"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "require": "./tsconfig.json"
    },
    "./tsconfig-vue.json": {
      "import": "./tsconfig-vue.json",
      "require": "./tsconfig-vue.json"
    },
    "./eslint": {
      "import": "./src/baseEslint.js",
      "require": "./src/baseEslint.js"
    },
    "./style.css": {
      "import": "./style/devkit.css",
      "require": "./style/devkit.css"
    }
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^24.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.4",
    "vue-router": "4"
  },
  "peerDependencies": {
    "@formkit/core": ">=1.6.9"
  },
  "dependencies": {
    "@devkitvue/apiclient": "workspace:*",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-turbo": "^2.5.4"
  }
}
