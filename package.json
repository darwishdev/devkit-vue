{
  "name": "@devkit",
  "private": true,
  "scripts": {
    "add": "pnpm",
    "setup-deps": "turbo run setup-deps",
    "sync-api": "turbo run sync-api",
    "build": "turbo run build",
    "build:datalist": "turbo run build --filter=@devkitvue/datalist",
    "build:filemanager": "turbo run build --filter=@devkitvue/filemanager",
    "build:form": "turbo run build --filter=@devkitvue/form",
    "lint": "turbo run lint",
    "dev:admin": "turbo run dev --filter=@devkitvue/admin-example",
    "dev:docs": "turbo run dev --filter=@devkitvue/docs",
    "test": "turbo run test",
    "dev": "turbo run dev --parallel",
    "view-report": "turbo run view-report"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "lodash.get": "*",
      "lodash.isequal": "*"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "jiti": "^2.4.2",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.0",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "globals": "^16.2.0"
  }
}
